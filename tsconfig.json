{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "ES2018",
    "sourceMap": false,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "strict": true,
    "declaration": true,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false,
    "lib": [
      "esnext"
    ]
  },
  "exclude": [
    "node_modules/",
    "dist/"
  ]
}

